#include <iostream>
#include <string>
using namespace std;

int main() {
    string names[10] = {"Ram","RAJA","Arun","Raj","Javascript","python","jan"};
    
    for( int i =0; i< sizeof(names)/sizeof(names[0]); i++){
        
        if(names[i].length() >=4){
            cout << "Names : " << names[i] <<"\n";
        }
    }

    return 0;
}
